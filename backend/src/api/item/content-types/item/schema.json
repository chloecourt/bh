{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "slug": {
      "type": "uid"
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "XS",
        "S",
        "M",
        "L",
        "XL",
        "XXL"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "productTitle": {
      "type": "string"
    }
  }
}
